{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ursa/src",
  "monorepo": true,
  "root": "apps/ursa",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/ursa/tsconfig.app.json"
  },
  "projects": {
    "ursa": {
      "type": "application",
      "root": "apps/ursa",
      "entryFile": "main",
      "sourceRoot": "apps/ursa/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ursa/tsconfig.app.json"
      }
    },
    "forest": {
      "type": "application",
      "root": "apps/forest",
      "entryFile": "main",
      "sourceRoot": "apps/forest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/forest/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "calculator": {
      "type": "library",
      "root": "libs/calculator",
      "entryFile": "index",
      "sourceRoot": "libs/calculator/src",
      "compilerOptions": {
        "tsConfigPath": "libs/calculator/tsconfig.lib.json"
      }
    },
    "forage": {
      "type": "application",
      "root": "apps/forage",
      "entryFile": "main",
      "sourceRoot": "apps/forage/src",
      "compilerOptions": {
        "tsConfigPath": "apps/forage/tsconfig.app.json"
      }
    },
    "recurly": {
      "type": "library",
      "root": "libs/recurly",
      "entryFile": "index",
      "sourceRoot": "libs/recurly/src",
      "compilerOptions": {
        "tsConfigPath": "libs/recurly/tsconfig.lib.json"
      }
    },
    "easypost": {
      "type": "library",
      "root": "libs/easypost",
      "entryFile": "index",
      "sourceRoot": "libs/easypost/src",
      "compilerOptions": {
        "tsConfigPath": "libs/easypost/tsconfig.lib.json"
      }
    }
  }
}